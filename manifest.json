{
  "manifest_version": 2,
  "name": "Swag Tabs",
  "description": "This extension lets you search your tabs with swag.",
  "version": "0.1",

  "background": {
    "scripts": ["thirdparty/jquery-2.1.3.min.js", "thirdparty/fuzzy.js", "background.js"]
  },

  // Puts icons in the toolbar (right of the address bar)
  "browser_action": {
    "default_title": "Swag Tabs",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs"
  ],
  // Spent a day and a half figuring security policy out
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}