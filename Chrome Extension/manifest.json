{
	"manifest_version": 2,
  "name": "Swag Alerts",
  "description": "This extension alerts you with the maximum level of swag."
  "version": "1.0",

  //puts icons in the toolbar (right of the address bar)
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}